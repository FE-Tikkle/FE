{
  "manifest_version": 3,
  "name": "First-test",
  "version": "1.0.0",
  "description": "",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["scripting"],
  "host_permissions": ["https://*/*", "http://*/*"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["kakao.js"]
    },
    {
      "content_security_policy": "script-src 'self' https://developers.kakao.com https://t1.kakaocdn.net; object-src 'self'"
    }
  ]
}
