{
  "manifest_version": 3,
  "name": "First-test",
  "version": "1.0.0",
  "description": "",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["scripting"],
  "host_permissions": ["http://localhost/*"],
  "content_scripts": [
    {
      "matches": ["http://localhost/*"],
      "js": ["content.js"]
    }
  ]
}
